{% extends 'base.html.twig' %}

{% block body %}
    <h1>Business</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ business.id }}</td>
            </tr>
            <tr>
                <th>Usersid</th>
                <td>{{ business.usersId }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ business.name }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ business.address }}</td>
            </tr>
            <tr>
                <th>Landmark</th>
                <td>{{ business.landmark }}</td>
            </tr>
            <tr>
                <th>Zip</th>
                <td>{{ business.zip }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ business.phone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ business.email }}</td>
            </tr>
            <tr>
                <th>Website</th>
                <td>{{ business.website }}</td>
            </tr>
            <tr>
                <th>Fax</th>
                <td>{{ business.fax }}</td>
            </tr>
            <tr>
                <th>Tollfree</th>
                <td>{{ business.tollFree }}</td>
            </tr>
            <tr>
                <th>About</th>
                <td>{{ business.about }}</td>
            </tr>
            <tr>
                <th>Workinghours</th>
                <td>{{ business.workingHours }}</td>
            </tr>
            <tr>
                <th>Employees</th>
                <td>{{ business.employees }}</td>
            </tr>
            <tr>
                <th>Entity</th>
                <td>{{ business.entity }}</td>
            </tr>
            <tr>
                <th>Size</th>
                <td>{{ business.size }}</td>
            </tr>
            <tr>
                <th>Turnover</th>
                <td>{{ business.turnOver }}</td>
            </tr>
            <tr>
                <th>Certification</th>
                <td>{{ business.certification }}</td>
            </tr>
            <tr>
                <th>Contactperson</th>
                <td>{{ business.contactPerson }}</td>
            </tr>
            <tr>
                <th>Ratting</th>
                <td>{{ business.ratting }}</td>
            </tr>
            <tr>
                <th>Agree</th>
                <td>{{ business.agree }}</td>
            </tr>
            <tr>
                <th>Long</th>
                <td>{{ business.long }}</td>
            </tr>
            <tr>
                <th>Lat</th>
                <td>{{ business.lat }}</td>
            </tr>
            <tr>
                <th>Featured</th>
                <td>{{ business.featured }}</td>
            </tr>
            <tr>
                <th>Verified</th>
                <td>{{ business.verified }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ business.status }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if business.createdAt %}{{ business.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{% if business.updatedAt %}{{ business.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Viewed</th>
                <td>{% if business.viewed %}{{ business.viewed|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('home-service-provider_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('home-service-provider_edit', { 'id': business.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
